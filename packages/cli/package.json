{
  "name": "larch-cli",
  "version": "1.0.0",
  "description": "Larch CLI",
  "main": "dist/index.js",
  "scripts": {
    "build": "node scripts/version.mjs && rm -rf ./dist && tsc",
    "start": "node dist/index.js",
    "lint": "eslint --ext .ts .",
    "package": "pkg . -o ../../bins/larch",
    "package:linux": "pkg . -o ../../bins/larch-linux -t node18-linux-x64,node18-linux-arm64",
    "package:macos": "pkg . -o ../../bins/larch-macos -t node18-macos-x64",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "engines": {
    "node": ">=18"
  },
  "author": "Jasti Sri Radhe Shyam <radheshyam.jasti@zeeve.io>",
  "license": "GPL-3.0-or-later",
  "devDependencies": {
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "pkg": "^5.8.1",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "commander": "^10.0.0",
    "larch-backend": "1.0.0"
  }
}
